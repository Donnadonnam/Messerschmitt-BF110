  <!-- L'hélice gauche--> 
  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <name>HeliceGauche</name>
    <object-name>heliceG</object-name>
    <object-name>bolG</object-name>
    <object-name>propdiscG</object-name>
    <object-name>propblurG</object-name>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>propdiscG</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblurG</object-name>
    <condition>
      <and>
        <greater-than>
          <property>engines/engine[0]/rpm</property>
          <value>600.000</value>
        </greater-than>
        <less-than>
          <property>engines/engine[0]/rpm</property>
          <value>1500.001</value>
        </less-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
